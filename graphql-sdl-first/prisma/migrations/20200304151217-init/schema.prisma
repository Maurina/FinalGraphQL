datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Course {
  id             String   @default(cuid()) @id
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  title          String
  description    String
  imageURL       String
  date           String
}